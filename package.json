    "@uniswap/token-lists": "1.0.0-beta.34",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "dotenv": "^16.4.5",
    "graphql": "^16.9.0",
    "lodash": "^4.17.21",
    "node-fetch": "2.6.3",
    "rimraf": "^5.0.10",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
